{
  "name": "appium-manager",
  "version": "1.0.0",
  "description": "Manages Appium server instances",
  "main": "app.js",
  "scripts": {
    "devstart": "NODE_ENV=development node -r dotenv/config app.js",
    "prodstart": "NODE_ENV=production node -r dotenv/config app.js",
    "start": "NODE_ENV=production node_modules/.bin/pm2 start pm2.config.js && node_modules/.bin/pm2 monit",
    "dev": "NODE_ENV=development node_modules/.bin/pm2 start pm2.config.js && node_modules/.bin/pm2 monit",
    "dev-master": "cross-env NODE_ENV=development MASTER_DOMAIN= ADB_HOST= ADB_PORT= node -r dotenv/config app.js",
    "monitor": "node_modules/.bin/pm2 monit",
    "stop": "node_modules/.bin/pm2 stop app.js",
    "delete": "node_modules/.bin/pm2 delete all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jeffrey Yang",
  "license": "ISC",
  "dependencies": {
    "adbkit": "^2.11.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dockerode": "^2.5.8",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "pm2": "^3.5.1",
    "socket.io": "^2.2.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.9.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
