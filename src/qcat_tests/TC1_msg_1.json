{
    "messages": [
        {
            "description": [
                "Check in Log for \"Attach Request\" message. Check att_type = 2 ( Combined EPS/ IMSI attach).",
                "Check id_type = 1 (IMSI)  OR 6 (GUTI)",
                "Within \"Attach request\" under esm_msg_container message check  \"PDN Connectivity Request\".",
                "Check PDN_type = ipv4v6",
                "Check req_type = 1 (initial attach)",
                "Check esm_info_trans_flag = 1",
                "Under ms_network_capability container check GEA/1= 1, GEA/2=1 and GEA/3=1",
                "Check ISR supported = 1",
                "Under ms_class_mark2 container check a5_1_alg_sup = 0, a5_3_alg_sup = 1",
                "Check voice_domain_pref_incl=1",
                "Check voice_domain_pref_for-EUTRAN = 2 (IMS PS Voice Preferred and CS secondary)",
                "Check WB-AMR is supported by checking below codecs",
                "Under codecs [0], check bitmap[1] = 4",
                "Under codecs [1], check bitmap[0] = 31"
            ],
            "packet_type": "0xB0ED",
            "subtitle": "Attach request Msg",
            "fields": [
                {
                    "field_name": "att_type",
                    "expected_value": "2",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "id_type",
                    "expected_value": [
                        "1",
                        "6"
                    ],
                    "validation_type": "FIRST_WORD_ONE_OF",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "msg_type",
                    "regex": "esm_msg_container(?:.|\n)*(msg_type.*)",
                    "expected_value": "208",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "pdn_type",
                    "expected_value": "3",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "req_type",
                    "expected_value": "1",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "esm_info_trans_flag",
                    "expected_value": "1",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "GEA/1",
                    "expected_value": "1",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "GEA/2",
                    "expected_value": "1",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "GEA/3",
                    "expected_value": "1",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "ISR support",
                    "expected_value": "1",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "a5_1_alg_sup",
                    "expected_value": "0",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "a5_3_alg_sup",
                    "expected_value": "1",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "bitmap[1]",
                    "regex": "codecs\\[0](?:.|\n)*(bitmap\\[1].*)(?:.|\n)*codecs\\[1]",
                    "expected_value": "4",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "bitmap[0]",
                    "regex": "codecs\\[1](?:.|\n)*(bitmap\\[0].*)",
                    "expected_value": "31",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "voice_domain_pref_incl",
                    "expected_value": "1",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "voice_domain_pref_for_EUTRAN",
                    "expected_value": "2",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                }
            ]
        }
    ]
