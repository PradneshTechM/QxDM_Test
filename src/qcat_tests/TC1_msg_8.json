{
    "messages": [
        {
            "description": [
                "Check in log for \"ueCapabilityInformation\" message.",
                "Check AccessStratumRelease =  must match with device spec",
                "Check 'UE-Category = must match with device spec",
                "Check supportedBandListEUTRA",
                "Confirm bandEUTRA 3, bandEUTRA 7, bandEUTRA 20, bandEUTRA 1, bandEUTRA 38 is available.",
                "Check supportedBandListUTRA-FDD",
                "Confirm bandI, bandII,band V, bandVIII are available.",
                "Check supportedBandListGERAN",
                "Confirm gsm850, gsm900, gsm1800 and gsm1900  are avialbale.",
                "Check InterRAT-PS-HO-GERAN = FALSE",
                "Check DTM-R9 = supported",
                "Check e-RedirectionGERAN-R9 = supported",
                "Check e-RedirectionUTRA-R9 = supported",
                "Check Feature Group indicator value is  \"01011101 00001111 11111110 10000000 11000000 1000000 00000000 000000000\".",
                "Make sure UE capability info message size do not exceed 2000 bytes"
            ],
            "packet_type": "0xB0C0",
            "subtitle": "UL_DCCH / UECapabilityInformation",
            "fields": [
                {
                    "field_name": "accessStratumRelease",
                    "validation_type": "CHECK_SAVED",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "ue-Category",
                    "validation_type": "CHECK_SAVED",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "supportedBandListEUTRA",
                    "regex": "supportedBandListEUTRA(?:.|\n)*?{(?:.|\n)*?({(?:.|\n)*?})\r\n",
                    "expected_value": [
                        "bandEUTRA 3",
                        "bandEUTRA 7",
                        "bandEUTRA 20",
                        "bandEUTRA 1",
                        "bandEUTRA 38"
                    ],
                    "validation_type": "COLLECTION",
                    "search_2": "bandEUTRA",
                    "field_type": "COLLECTION",
                    "get_value": true
                },
                {
                    "field_name": "supportedBandListUTRA-FDD",
                    "regex": "supportedBandListUTRA-FDD(?:.|\n)*?{((?:.|\n)*?)}",
                    "expected_value": [
                        "bandI",
                        "bandII",
                        "bandV",
                        "bandVIII"
                    ],
                    "validation_type": "COLLECTION",
                    "field_type": "COLLECTION",
                    "get_value": true
                },
                {
                    "field_name": "supportedBandListGERAN",
                    "regex": "supportedBandListGERAN(?:.|\n)*?{((?:.|\n)*?)}",
                    "expected_value": [
                        "gsm850",
                        "gsm900",
                        "gsm1800",
                        "gsm1900"
                    ],
                    "validation_type": "COLLECTION",
                    "field_type": "COLLECTION",
                    "get_value": true
                },
                {
                    "field_name": "DTM-R9",
                    "expected_value": "supported",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "e-RedirectionGERAN-R9",
                    "expected_value": "supported",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "e-RedirectionUTRA-r9",
                    "expected_value": "supported",
                    "validation_type": "FIRST_WORD_EXACT",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "featureGroupIndicators",
                    "expected_value": "01011101 00001111 11111110 10000000 11000000 1000000 00000000 000000000",
                    "validation_type": "ANY_SUBSTRING",
                    "field_type": "VALUE",
                    "get_value": true
                },
                {
                    "field_name": "Msg Length",
                    "expected_value": "2000",
                    "validation_type": "NUMBER_COMPARISON",
                    "regex": "(Msg Length.*)",
                    "field_type": "VALUE",
                    "get_value": true
                }
            ]
        }
    ]
}